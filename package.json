{
  "name": "hoteldellunaserver",
  "version": "1.0.0",
  "description": "Hotel Del Luna API",
  "main": "app.js",
  "scripts": {
    "seed": "babel-node ./src/seed/seedRoom",
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "env NODE_ENV=development npm-run-all build server",
    "prod": "env NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": "12.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "babel-loader": "^8.1.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.26.0",
    "mongoose": "^5.9.17",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "stripe": "^8.63.0"
  },
  "devDependencies": {
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4"
  }
}
